<!doctype html>
<html class="no-js" lang="{{ request.locale.iso_code }}">
  
  <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/swiper@11/swiper-bundle.min.css">

  <!-- Custom Revolution Slider styling -->
  <style>
    .rev_slider_wrapper,.rev_slider{
      width:auto!important;max-width:none!important;margin:0 auto!important;
      display:flex!important;justify-content:center!important;overflow-x:auto!important;
      scroll-behavior:smooth!important;scroll-snap-type:x mandatory;position:relative!important
    }
    .rev_slider .tp-revslider-mainul li{
      position:relative!important;display:inline-flex!important;
      justify-content:center!important;align-items:center!important;scroll-snap-align:center
    }
    .rev_slider .tp-revslider-mainul img{
      width:auto!important;height:auto!important;max-width:none!important;
      display:block!important;margin:0 0px!important; /* adjust spacing */
    }
    .rev_slider_wrapper::-webkit-scrollbar{display:none!important}
    .rev_slider_wrapper::before,.rev_slider_wrapper::after{
      content:"";position:sticky;top:0;width:60px;height:100%;
      pointer-events:none;z-index:5
    }
    .rev_slider_wrapper::before{
      left:0;background:linear-gradient(to right,#fff,rgba(255,255,255,0))
    }
    .rev_slider_wrapper::after{
      right:0;background:linear-gradient(to left,#fff,rgba(255,255,255,0))
    }
  </style>
  <!-- End Custom Revolution Slider styling -->

  <head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width,initial-scale=1">
    <meta name="theme-color" content="">
    <link rel="canonical" href="{{ canonical_url }}">
    <link rel="preconnect" href="https://cdn.shopify.com" crossorigin>

    {%- if settings.favicon != blank -%}
      <link
        rel="icon"
        type="image/png"
        href="{{ settings.favicon | image_url: width: 32, height: 32 }}"
      >
    {%- endif -%}

    {%- unless settings.type_header_font.system? and settings.type_body_font.system? -%}
      <link rel="preconnect" href="https://fonts.shopifycdn.com" crossorigin>
    {%- endunless -%}

    <title>
      {{ page_title }}
      {%- if current_tags %}
        &ndash; tagged "{{ current_tags | join: ', ' }}"
      {%- endif -%}
      {%- if current_page != 1 %} &ndash; Page {{ current_page }}{% endif -%}
      {%- unless page_title contains shop.name %}
        &ndash; {{ shop.name -}}
      {%- endunless -%}
    </title>

    {% if page_description %}
      <meta name="description" content="{{ page_description | escape }}">
    {% endif %}

    {{ content_for_header }}

    {%- liquid
      render 'meta-tags'
      render 'js-bridge'
    -%}

    <script src="https://cdn.jsdelivr.net/npm/swiper@11/swiper-bundle.min.js"></script>

    <script src="{{ 'utils.js' | asset_url }}" defer></script>
    <script
      src="{{ 'polyfills-inert.bundle.min.js' | asset_url }}"
      type="module"
    ></script>
    <script
      src="{{ 'global.bundle.min.js' | asset_url }}"
      type="module"
    ></script>
    <script
      src="{{ 'quantity-stepper.js' | asset_url }}"
      type="module"
    ></script>
    {% if settings.quantity_style == 'dropdown' %}
      <script
        src="{{ 'quantity-dropdown.js' | asset_url }}"
        type="module"
      ></script>
    {% endif %}

    {% if template == 'cart' or settings.cart_type == 'drawer' %}
      <script src="{{ 'cart-items.js' | asset_url }}" type="module"></script>
      <script src="{{ 'cart-footer.js' | asset_url }}" type="module"></script>
      {% if settings.cart_notes_enable %}
        <script src="{{ 'cart-note.js' | asset_url }}" type="module"></script>
      {% endif %}
    {% endif %}

    <script src="{{ 'section-group-resize-observer.js' | asset_url }}" defer></script>

    {% if request.design_mode %}
      <script src="{{ 'cart-count.js' | asset_url }}" type="module"></script>
      <script
        src="{{ 'modules-masonry.bundle.min.js' | asset_url }}"
        type="module"
      ></script>
      <script
        src="{{ 'modules-slideshow-gallery.bundle.min.js' | asset_url }}"
        type="module"
      ></script>
      <script src="{{ 'tile.js' | asset_url }}" type="module"></script>
      <script src="{{ 'follow-mouse.js' | asset_url }}" type="module"></script>
      <script
        src="{{ 'label-overlay-on-hover.js' | asset_url }}"
        type="module"
      ></script>
      {% if settings.predictive_search_enabled %}
        <script
          src="{{ 'predictive-search.js' | asset_url }}"
          type="module"
        ></script>
      {% endif %}
      <script
        src="{{ 'theme-editor-support.js' | asset_url }}"
        type="module"
      ></script>
      <script
        src="{{ 'sections-promo-popup.bundle.min.js' | asset_url }}"
        type="module"
      ></script>
      <script
        src="{{ 'vendor-product.bundle.min.js' | asset_url }}"
        type="module"
      ></script>
      <script src="{{ 'product.js' | asset_url }}" type="module"></script>
      <script src="{{ 'video.js' | asset_url }}" type="module"></script>
      <script
        src="{{ 'vendor-store-availability.bundle.min.js' | asset_url }}"
        type="module"
      ></script>
      <script
        src="{{ 'store-availability.js' | asset_url }}"
        type="module"
      ></script>
      <script src="{{ 'product-model.js' | asset_url }}" type="module"></script>
      <script
        src="{{ 'modules-slideshow.bundle.min.js' | asset_url }}"
        type="module"
      ></script>
      <script
        src="{{ 'modules-carousel.bundle.min.js' | asset_url }}"
        type="module"
      ></script>
      <script
        src="{{ 'product-media-gallery.js' | asset_url }}"
        type="module"
      ></script>
      <script src="{{ 'zoom.js' | asset_url }}" type="module"></script>
      <script src="{{ 'video.js' | asset_url }}" type="module"></script>
      <script src="{{ 'html5-video.js' | asset_url }}" type="module"></script>
      <script src="{{ 'rte.js' | asset_url }}" type="module"></script>
      <script
        src="{{ 'gift-card-recipient.js' | asset_url }}"
        type="module"
      ></script>
    {% endif %}

    {% if settings.cart_type == 'drawer' %}
      <script
        src="{{ 'vendor-cart.bundle.min.js' | asset_url }}"
        type="module"
      ></script>
    {% endif %}

    {% if template contains 'customers' %}
      {% comment %}theme-check-disable ParserBlockingScript{% endcomment %}
      {{ 'shopify_common.js' | shopify_asset_url | script_tag }}
      {{ 'customer_area.js' | shopify_asset_url | script_tag }}
      {% comment %}theme-check-enable ParserBlockingScript{% endcomment %}
    {% endif %}

    <script>
      window.onYouTubeIframeAPIReady = () => {
        document.body.dispatchEvent(new CustomEvent('youtubeiframeapiready'));
      };
    </script>

    {%- render 'css-bridge' -%}

    {{ 'base.bundle.css' | asset_url | stylesheet_tag }}
    {% include 'add_header_assets' %}
  </head>

  <body>
    {%- if settings.enable_sidebar -%}
      {%- if settings.sidebar_desktop_layout == 'screen_height' -%}
        <div id="mainGridWrapper" class="main-site-grid">
          <div
            class="main-site-grid--sidebar text-scheme-text bg-scheme-background hide-scrollbars"
            data-color-scheme="{{ settings.sidebar_color_scheme | default: 'primary' }}"
          >
            {%- sections 'aside-group' -%}
          </div>
          <div class="main-site-grid--container relative">
            {%- sections 'header-group' -%}
            {%- if template == 'index' -%}
              <div id="heroGroupWrapper">
                {%- sections 'hero-group' -%}
              </div>
            {%- endif %}
            <main id="MainContent" role="main" tabindex="-1">
              <div>
                {{ content_for_layout }}
              </div>
            </main>
            {%- sections 'footer-group' -%}
          </div>
        </div>
      {%- else -%}
        {%- comment -%}
          Sidebar under header and hero
        {%- endcomment -%}
        {%- sections 'header-group' -%}
        {%- if template == 'index' -%}
          <div id="heroGroupWrapper">
            {%- sections 'hero-group' -%}
          </div>
        {%- endif %}
        <div id="mainGridWrapper" class="main-site-grid">
          <div class="main-site-grid--sidebar hidden lg:block hide-scrollbars">
            {%- sections 'aside-group' -%}
          </div>
          <main
            id="MainContent"
            class="main-site-grid--container max-lg:max-w-screen"
            role="main"
            tabindex="-1"
          >
            <div>
              {{ content_for_layout }}
            </div>
          </main>
        </div>
        {%- sections 'footer-group' -%}
      {%- endif -%}
    {%- else -%}
      {%- sections 'header-group' -%}
      {%- if template == 'index' -%}
        <div id="heroGroupWrapper">
          {%- sections 'hero-group' -%}
        </div>
      {%- endif %}
      <main
        id="MainContent"
        class="main-site-grid--container"
        role="main"
        tabindex="-1"
      >
        <div>
          {{ content_for_layout }}
        </div>
      </main>
      {%- sections 'footer-group' -%}
    {%- endif -%}

    {%- sections 'overlay-group' -%}

    <div
      id="screenreader-announce"
      class="sr-only"
      aria-live="polite"
      role="status"
    ></div>

    <ul hidden>
      <li id="a11y-refresh-page-message">
        {{ 'general.accessibility.refresh_page' | t }}
      </li>
      <li id="a11y-selection-message">
        {{ 'general.accessibility.selection_help' | t }}
      </li>
    </ul>

    {% comment %}
        Modal container
      {%- sections 'overlay-group' -%}
    {% endcomment %}
    {% liquid
      render 'left-drawer'
      render 'right-drawer'
      render 'quick-view-drawer'
      render 'popup'
      render 'modal'
      render 'promo'
      render 'product-quick-view'
      render 'button-sizer'
    %}

    <script src="{{ 'modals.js' | asset_url }}" type="module"></script>

    {% if settings.cart_type == 'drawer' and template != 'cart' %}
      {% render 'cart-drawer' %}
    {% endif %}

    <script
      src="{{ 'alpine.bundle.min.js' | asset_url }}"
      type="module"
    ></script>

    <script
      src="{{ 'load-images.js' | asset_url }}"
      type="module"
    ></script>
    {% include 'add_footer_assets' %}
  </body>
</html>
